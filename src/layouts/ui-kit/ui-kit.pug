include /common/logo/logo

block variables

doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title= title
  body.ui-kit-page(
    class=modifierClass ? `ui-kit-page_${modifierClass}` : ''
  )
    header.ui-kit-header
      .ui-kit-header__logo
        +logo-mini
    .container
      div(class=pageClass)
        block content